{
	"name": "chillerlan/php-curl",
	"description": "A tiny cURL wrapper. PHP 7+",
	"homepage": "https://github.com/chillerlan/php-curl",
	"license": "MIT",
	"type": "library",
	"keywords": [
		"curl", "wrapper", "multi", "url", "extract"
	],
	"authors": [
		{
			"name": "Smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		}
	],
	"support": {
		"issues": "https://github.com/chillerlan/php-curl/issues",
		"source": "https://github.com/chillerlan/php-curl"
	},
	"minimum-stability": "stable",
	"require": {
		"php": ">=7.0.7",
		"chillerlan/php-traits": "^1.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.5"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\TinyCurl\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\TinyCurlTest\\": "tests/",
			"Example\\": "examples/"
		}
	}
}
